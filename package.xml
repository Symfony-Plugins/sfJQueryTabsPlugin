 <notes>-</notes>
 <contents>
    <dir name="/">
      <dir name="config">
	      <dir name="examples">
	        <file name="jquerytabs.yml" role="data" />
	      </dir>
      </dir>
      <dir name="modules">
        <dir name="sfJQueryTabs">
          <dir name="actions">
            <file name="actions.class.php" role="data" />
          </dir>
          <dir name="lib">
            <file name="sfJQueryTabs.class.php" role="data" />
          </dir>
          <dir name="templates">
            <file name="indexSuccess.php" role="data" />
            <file name="layout.php" role="data" />
          </dir>
        </dir>
      </dir>
      <dir name="web">
        <dir name="ui.tabs">
          <file name="jquery-1.2.3.pack.js" role="data" />
          <file name="ui.tabs.ext.js" role="data" />
          <file name="ui.tabs.ext.min.js" role="data" />
          <file name="ui.tabs.ext.pack.js" role="data" />
          <file name="ui.tabs.js" role="data" />
          <file name="ui.tabs.min.js" role="data" />
          <file name="ui.tabs.pack.js" role="data" />
          <file name="ui.tabs.css" role="data" />
          <file name="loading.gif" role="data" />
          <file name="tab.png" role="data" />
        </dir>
      </dir>
      <file name="LICENSE" role="data" />
      <file name="README" role="data" />
      <file name="screenshot.jpg" role="data" />
    </dir>
 </contents>
